# Arc Testnet RPC
ARC_TESTNET_RPC_URL=https://rpc.testnet.arc.network

# Deployer/Admin private key
# The deployer wallet will be set as the admin of the Treasury contract
PRIVATE_KEY=0x...

# Backend wallet (for marking spends executed/failed)
# This wallet will be added as a backend operator during deployment
BACKEND_PRIVATE_KEY=0x...
BACKEND_WALLET_ADDRESS=0x...

# Contract addresses (filled after deployment)
TREASURY_CONTRACT_ADDRESS=

# Circle Gateway configuration (Arc Testnet)
# Gateway Wallet address - used for unified USDC balance
GATEWAY_WALLET_ADDRESS=0x0077777d7EBA4688BDeF3E311b846F25870A19B9
# Gateway Minter address - used for cross-chain USDC minting
GATEWAY_MINTER_ADDRESS=0x0022222ABE238Cc2C7Bb1f21003F0a260052475B

# Blockscout API Key (optional, for contract verification)
# Get your API key from https://testnet.arcscan.app/account/api-key
BLOCKSCOUT_API_KEY=

